<div class="container top-offset">
  <div class="row">
    <form method="post" class="form-horizontal">
    


      <div class="row">
        <div class="p_img col-md-6">
            <div class="inputImage">
              <i class="fa fa-user"></i>
            </div>
        </div>
        <div class="p_img col-md-6">


                <div class="form-group">
                  <label for="inputName" class="col-sm-1 control-label">Name</label>
                  <div class="col-sm-10">
                    <input type="text" name="inputName" class="form-control" id="inputName" placeholder="Name">
                  </div>
                </div>

                <div class="form-group">
                  <label for="inputTwitter" class="col-sm-1 control-label">Twitter</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="inputTwitter" name="Twitter" id="inputTwitter" placeholder="Twitter">
                  </div>
                </div>

                <div class="form-group">
                  <label for="inputSkype" class="col-sm-1 control-label">Skype</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="inputSkype" id="inputSkype" placeholder="Skype">
                  </div>
                </div>

                <div class="form-group">
                  <label for="age" class="col-sm-1 control-label">Age</label>
                  <div class="col-sm-10">
                    <input type="number" class="form-control" name="age" id="age" placeholder="0">
                  </div>
                </div>

                <div class="form-group">
                  <label for="targetFunds" class="col-sm-1 control-label">Funds Target</label>
                  <div class="col-sm-10">
                    <input type="number" class="form-control" name="targetFunds" id="targetFunds" placeholder="0">
                  </div>
                </div>

                <div class="form-group">
                  <label for="inputStory" class="col-sm-1 control-label">Story</label>
                  <div class="col-sm-10">
                    <textarea type="text" class="form-control" name="inputStory" id="inputStory"></textarea>
                  </div>
                </div>

                <div class="form-group">
                  <div class="col-sm-offset-1 col-sm-10">
                    <button type="submit" class="btn btn-default btn-broad">Nominate</button>
                  </div>
                </div>

        </div>

      </div>



    </form>
  </div>
</div>

